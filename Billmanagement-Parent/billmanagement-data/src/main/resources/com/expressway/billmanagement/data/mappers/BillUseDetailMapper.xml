<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 票据使用明细 -->
<mapper namespace="com.expressway.billmanagement.data.mappers.BillUseDetailMapper">
	<resultMap id="resultMap" type="BillUseDetail">
		<id property="employeeNo" column="employeeNo" />
		<result property="employeeName" column="employeeName" />
		<result property="enteName" column="enteName" />
		<result property="orderNo" column="orderNo" />
		<result property="orderDate" column="orderDate" />
		<result property="totalFare" column="totalFare" />
	</resultMap>

	<select id="findByCondition" resultMap="resultMap" parameterType="ConditionFiled">
		select * from bill_use_detail t
	</select>
</mapper>