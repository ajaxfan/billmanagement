<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 票据使用明细 -->
<mapper namespace="com.expressway.billmanagement.data.mappers.BillUseDetailMapper">
	<resultMap id="resultMap" type="BillUseDetail">
		<result property="dateRange" column="dateRange" />
		<result property="employeeNo" column="employeeNo" />
		<result property="remark" column="remark" />
		<result property="useBegin" column="useBegin" />
		<result property="useEnd" column="useEnd" />
		<result property="useCount" column="useCount" />
		<result property="sellBegin" column="sellBegin" />
		<result property="sellEnd" column="sellEnd" />
		<result property="sellCount" column="sellCount" />
		<result property="invalidBegin" column="invalidBegin" />
		<result property="invalidEnd" column="invalidEnd" />
		<result property="invalidCount" column="invalidCount" />
		<result property="subtotalBegin" column="subtotalBegin" />
		<result property="subtotalEnd" column="subtotalEnd" />
		<result property="subtotalCount" column="subtotalCount" />
		<result property="balanceBegin" column="balanceBegin" />
		<result property="balanceEnd" column="balanceEnd" />
		<result property="balanceCount" column="balanceCount" />
	</resultMap>

	<select id="findByCondition" resultMap="resultMap" parameterType="ConditionFiled">
		select * from bill_use_detail t
	</select>
</mapper>